<?xml version="1.0" encoding="ISO-8859-1"?>
<panel name="vaccineAdministrationPanel" weightx="1" weighty="1">
    <panel border="true" bottom="5" columns="1" gap="5" left="5" name="vaccineListPanel" right="5" text="i18n.medic.appointments.series" top="5" weightx="1" weighty="1" icon="/icons/calendar_view_month.png">
        <list name="vaccineList" weightx="1" weighty="1" action="vaccineListSelectionChanged()"/>
        <panel columns="1" gap="5" name="vaccineButtonPanel" weightx="1">
            <button name="addVaccineButton" text="i18n.medic.appointments.series.add" weightx="1" action="addVaccine()" icon="/icons/calendar_add.png"/>
            <button name="removeVaccineButton" text="i18n.medic.appointments.series.remove" weightx="1" action="removeVaccine()" icon="/icons/calendar_delete.png"/>
        </panel>
    </panel>
    <panel bottom="5" columns="1" left="5" name="vaccineActionPanel" right="5" top="5" weightx="3" weighty="1"/>
</panel>