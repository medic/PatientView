<?xml version="1.0" encoding="ISO-8859-1"?>
<panel columns="2" gap="5" name="patientVaccinePanel" weightx="1" weighty="1" top="5" bottom="5" left="5" right="5">
    <panel border="true" bottom="5" columns="1" gap="5" left="5" name="vaccineListPanel" right="5" text="Patient's Appointment Series" top="5" weightx="1" weighty="1" icon="/icons/calendar_small.png">
        <list name="vaccineList" weightx="1" weighty="1" action="vaccineListSelectionChanged()"/>
        <panel columns="1" gap="5" name="vaccineButtonPanel" weightx="1">
            <button action="scheduleVaccineClicked()" name="scheduleVaccineButton" text="Enroll in Appointment Series" weightx="1" icon="/icons/calendar_add.png"/>
            <button action="removeVaccineClicked()" name="removeVaccineButton" text="Remove Appointment Series" weightx="1" icon="/icons/calendar_delete.png"/>
        </panel>
    </panel>
    <panel columns="1" gap="5" name="scheduledDosePanel" weightx="4" weighty="1">
        <panel name="scheduledDoseTablePanel" weightx="1" weighty="1"/>
        <panel gap="5" name="doseButtonPanel">
            <button action="rescheduleDoseClicked()" name="rescheduleDoseButton" text="Reschedule" icon="/icons/calendar_refresh.png"/>
            <button action="administerDoseClicked()" name="adminsterDoseButton" text="Complete" icon="/icons/calendar_complete.png"/>
            <label weightx="1"/>
            <button action="rescheduleAllDosesClicked()" name="rescheduleAllButton" text="Reschedule All" icon="/icons/calendar_refresh_all.png"/>
        </panel>
    </panel>
</panel>